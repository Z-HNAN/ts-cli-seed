{
  "name": "cli-ts-seed",
  "version": "0.0.1",
  "description": "cli seed",
  "author": "",
  "bin": {
    "cli-ts-seed": "./dist/bin.js"
  },
  "scripts": {
    "clean": "rimraf ./dist ./coverage ./types",
    "lint": "eslint src test --ext js,ts",
    "check-types": "tsc --build tsconfig.eslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "tsc -w",
    "build": "tsc",
    "pretest": "npm run build",
    "prepublishOnly": "npm run clean && npm run lint && npm run check-types && npm test"
  },
  "files": [
    "dist",
    "types"
  ],
  "main": "dist/bin.js",
  "types": "types/index.d.ts",
  "sideEffects": false,
  "dependencies": {
    "axios": "^0.24.0",
    "commander": "^8.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@commitlint/cli": "^10.0.0",
    "@commitlint/config-conventional": "^10.0.0",
    "@types/commander": "^2.12.2",
    "@types/jest": "^26.0.1",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "husky": "4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.1.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint .commitlintrc.js -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --ext js,ts",
      "tsc --noEmit --allowJs"
    ],
    "test/**/*.{js,ts}": [
      "eslint --ext js,ts",
      "tsc --noEmit --allowJs"
    ]
  },
  "license": "UNLICENSED"
}
